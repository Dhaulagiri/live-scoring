<h3 class="text-center">{{fullName}}</h3>
<dl class="dl-horizontal">
  <dt>First Name</dt>
  <dd>{{firstName}}</dd>
  <dt>Middle Name</dt>
  <dd>{{middleName}}</dd>
  <dt>Last Name</dt>
  <dd>{{lastName}}</dd>
  <dt>Gender</dt>
  <dd>{{gender}}</dd>
  <dt>Age</dt>
  <dd>{{dob}}</dd>
</dl>
<button type="button" class="btn btn-default edit-button" {{action "edit" this}}>Edit</button>
<button type="button" class="btn btn-danger delete-button" {{action "delete" this}}>Delete</button>
